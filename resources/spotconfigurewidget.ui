   <property name="toolTip">
    <string>Spot aging profile</string>
   </property>
   <property name="whatsThis">
    <string>&lt;html&gt;&lt;head/&gt;&lt;body&gt;&lt;p&gt;Tells whether the spot growth and decay timewise profile is trapezoidal or triangular. Setting NOMAX=1 eliminates the interval of constant size that otherwise exists at spot maximum (spot aging profiles then become triangular). Spot aging profiles are trepezoidal for NOMAX=0.&lt;/p&gt;&lt;/body&gt;&lt;/html&gt;</string>
   </property>
   <property name="text">
    <string>NOMAX</string>
   </property>
